<div class="modal fade" id="tradein" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Закажите оценку Вашего автомобиля в TradeIn:</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="container">
          <div class="row">
            <div class="col-md-12">
              {{ runSnippet('Form', {
              formid: 'FormTradeIn',
              path: '/templates/rofl/partials/forms/tradeIn/',
              extension: 'twig'
              }) | raw }}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
 

  <div id="trade-in-popup" class="popup-fb popup_trade-in" style="display: none;">
  <div class="popup__header popup__header_trade-in">
    <div>
      <div class="popup__heading">Получите лучшее предложение по&nbsp;Trade&nbsp;In</div>
      <div class="popup__subheading">Оценим ваш автомобиль за 10 минут</div>
    </div>
    <div>
      <div class="popup__descr">Выгода до</div>
      <div class="popup__nums">250 000 рублей</div>
    </div>
    <div class="popup__auto">
      <img src="/templates/autosalon/images/tradein-popup-car.png" alt="">
    </div>
    <img class="popup__header-pic" src="/templates/autosalon/images/money-popup.png" alt="">
  </div>
  <div class="popup__content">
    <div role="form" class="wpcf7" id="" lang="ru-RU" dir="ltr">
      {{ runSnippet('Form', {
      formid: 'FormCallback',
      path: '/templates/autosalon/partials/forms/feedback_popup/',
      extension: 'twig'
      }) | raw }}
    </div>
  </div>
</div>
<div class="sticky">
  <div class="sticky__close"></div>
  <a href="#trade-in-popup" data-fancybox="" class="sticky__plate sticky__plate_tradin">
    <img src="/templates/autosalon//images/sticky-car.png" alt="">
    <div class="sticky__inner">
      <div class="sticky__circle"></div>
      <div class="sticky__caption">Пора обновляться!</div>
      <div class="sticky__descr">Максимальная выгода от Trade-In</div>
      <div class="button button_red button_sm"><span>250 000 руб</span></div>
    </div>
  </a>
</div>
<link rel="stylesheet" href="/templates/autosalon/css/popup.css">
    <link rel="stylesheet" href="/templates/autosalon/css/plates.css">
    
<div class="bottom-page-line py-4 d-md-none">
        <div class="container">
            <p><span>Ликвидация склада 2022 года.</span> Успей получить свою выгоду</p>
            <div class="image">
                <img src="/templates/rofl/img/image.png">
            </div>
            <a class="close"></a>
        </div>
</div>
<style type="text/css">
.bottom-page-line {
  position: fixed;
    bottom: 0px;
    right: 0%;
    width: 100%;
  background: #DC6868;
  box-shadow: 0px 0px 25px rgba(0, 0, 0, 0.1);
  color: #fff;
}
.bottom-page-line .container {
  position: relative;
}
.bottom-page-line p {
  font-size: 16px;
    line-height: 20px;
    padding-right: 55%;
    z-index: 3;
    position: relative;
}
.bottom-page-line p span {
  font-weight: 700;
}
.bottom-page-line .close {
  position: absolute;
    top: 0%;
    transform: translateY(-50%);
    right: 16px;
    width: 24px;
    height: 24px;
    background: url(/templates/rofl/img/close.svg);
    opacity: 0;
}
.bottom-page-line .image {
  position: absolute;
    top: 50%;
    transform: translateY(-50%);
    right: 10px;
    pointer-events: none;
    z-index: 2;
    max-width: 50%;
}
</style>
<script>
  document.addEventListener("DOMContentLoaded", function() {
    var closeBtn = document.querySelector(".sticky__close");
    closeBtn.addEventListener("click", function() {
      var sticky = document.querySelector(".sticky");
      sticky.style.display = "none";
    });
  });

  </script>