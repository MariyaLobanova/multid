{% set bank_logos = runSnippet('DocInfo', {field: 'images', docid: 41118}) | json%}
{% set headingbank = runSnippet('DocInfo', {field: 'longtitle', docid: 41118}) | raw %}

<div class="partners-block mb-5">
    <h2 class="mb-4">{{headingbank}}</h2>
    <div class="list">
        {% for item in bank_logos["fieldValue"] %}
            <div class="item"><img src="{{item.image}}"></div>
        {% endfor %}
    </div>
</div>